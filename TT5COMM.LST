         4                      		;THIS IS A PROGRAM FOR THE TT5
         5                      		;TERMINAL TO COMMUNICATE WITH THE
         6                      		;PROLOG 7842-1 Z80 CPU CARD.
         7  0000
         8                      		;       IN THE MAIN PROGRAM THAT USES THIS MODULE IT WILL
         9                      		;  NEED TO CALL CHREC.
        10                      		;  	THIS WILL CALL THE ROUTINE AND CHECK IF THE RECEIVED
        11                      		;  FLAG IS SET, AND IF SO WILL PERFORM THE PROCESS.  THE
        12                      		;  RECEIVED FLAG WILL BE SET WHEN INFORMATION IS COMING IN
        13                      		;  FROM THE TT5 TERMINAL.
        14                      		;	THE PROGRAM IS MADE TRANSPARENT IN THAT IF THE
        15                      		;  TRANSMIT BUFFER FILLS, A FLAG IN THE SIO IS SET.
        16                      		;  THE LOCATION WILL BE PLACED IN A JUMP REGISTER SO
        17                      		;  THE NEXT TIME THE PROGRAM IS CALLED, AND IF THE TRANSMIT
        18                      		;  BUFFER FLAG IS RESET, THE LOCATION WILL BE RETURNED TO AND
        19                      		;  THE DESIRED TASK WILL CONTINUE.	
        20                      		;	IT IS RECOMMENDED THAT THE PROGRAM BE CALLED ON
        21                      		;  A TIMED BASIS SUCH AS EVERY 10mS.
        22  0000
        23  0000
        24                      	NAME 	COMM
        25  0000
        26                      	GLOBAL	VAL1,VAL2,CHREC,SIOI,VAL10
        27                      	EXTRN	DISPLAY,DISPLAYC,ESIRLY
        28  0000
        29                      	ORG	1000H
        30  1000
        31                      	;*********************MACRO DEFINITIONS******************
        32  1000
        33                      WRITE		MACRO	#X1,#X2,#X3,#X4
        34
        35                      	LD	HL,W#$YM
        36                      	IFC	#X4,C
        37                      	CALL	DISPLAYC
        38                      	ELSE
        39                      	CALL	DISPLAY
        40                      	ENDC
        41                      	JP	W2#$YM
        42                      W#$YM	IFC	#X3,C
        43                      	DB	12-((W2#$YM-W3#$YM)/2)+((#X2-1)*24)
        44
        45                      	ELSE
        46                      	DB	(#X3-1)+((#X2-1)*24)
        47                      	ENDC
        48                      W3#$YM	DEFM	'#X1'
        49                      W2#$YM	NOP
        50                      	ENDM
